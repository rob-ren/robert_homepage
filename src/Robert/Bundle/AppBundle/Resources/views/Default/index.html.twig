<body class="push-body">
<header id="main-navigation">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12"><a href="#" class="navbar-brand pull-right"></a>
                <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left">
                    <a href="#" class="push_nav_brand">
                        {% trans %}homepage.firstname{% endtrans %}{% trans %}homepage.lastname{% endtrans %}
                    </a>
                    <ul class="push_nav">
                        <li><a class="active scroll" href="#home">{% trans %}homepage.home{% endtrans %}</a></li>
                        <li><a class="scroll" href="#aboutme">{% trans %}homepage.about_me{% endtrans %}</a></li>
                        <li><a class="scroll" href="#mywork">{% trans %}homepage.my_life{% endtrans %}</a></li>
                        <li><a class="scroll" href="#contactmesec">{% trans %}homepage.contact_me{% endtrans %}</a></li>
                    </ul>
                    <ul class="icons">
                        <li><a href="https://www.facebook.com/profile.php?id=100004002800303" target="_blank"
                               class="fa"><i class="fa fa-fw fa-facebook"></i></a></li>
                        <li><a href="https://www.linkedin.com/in/xubo-ren-149266ab/" target="_blank" class="fa"><i
                                        class="fa fa-fw fa-linkedin"></i></a></li>
                        <li><a href="https://github.com/robertren1" target="_blank" class="fa gh"><i
                                        class="fa fa-fw fa-github"></i></a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </div>
</header>
<div class="container-fluid">
    <div class="main-button left">
        <button class="toggle-menu menu-left push-body"><span></span> <span></span> <span></span></button>
    </div>
</div>

<!-- Info-->
<div id="home" class="row section">
    <div class="container info text-center">
        <div class="row">
            <div class="col-xs-12 text-center">
                <div class="info_detail">
                    <div class="zoom"><img src="assets/images/robert.jpg" alt="alex">
                        <div class="zoom-overlay"></div>
                    </div>
                    <h3>{% trans %}homepage.firstname{% endtrans %} {% trans %}homepage.lastname{% endtrans %}</h3>
                    <p>{% trans %}homepage.role{% endtrans %}</p>
                    <!-- Social Icons -->
                    <div class="icons">
                        <ul>
                            <li><a href="https://www.facebook.com/profile.php?id=100004002800303" target="_blank"
                                   class="fa fb"><i class="fa fa-fw fa-facebook"></i></a></li>
                            <li><a href="https://www.linkedin.com/in/xubo-ren-149266ab/" target="_blank"
                                   class="fa in"><i class="fa fa-fw fa-linkedin"></i></a></li>
                            <li><a href="https://github.com/robertren1" target="_blank" class="fa gh"><i
                                            class="fa fa-fw fa-github"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="button"><a href="#contactmesec" data-target="contactmesec" class="btn active scroll">{% trans %}
                homepage.contact_me{% endtrans %}</a>
            <a href="#aboutme" data-target="aboutme" class="btn scroll">{% trans %}homepage.about_me{% endtrans %}</a>
        </div>
    </div>
</div>

<!-- About -->
<div id="aboutme" class="row section">
    <div class="container career text-center">
        <div class="row">
            <div class="col-xs-12 text-center">
                <div class="animate fadeInRight">
                    <div class="heading">
                        <h1>{% trans %}homepage.my_career_time_line{% endtrans %}</h1>
                        <p>{% trans %}homepage.career_content{% endtrans %}</p>
                    </div>
                </div>
                <div class="col-md-12 text-center">
                    <div id="text-demo" class="owl-carousel">
                        <div class="item text-center">
                            <h2>{% trans %}homepage.title.1{% endtrans %}</h2>
                            <em>{% trans %}homepage.company.1{% endtrans %},<span> 2011-2012</span></em>
                            <p>{% trans %}homepage.job_description.1{% endtrans %}</p>
                        </div>
                        <div class="item text-center">
                            <h2>{% trans %}homepage.title.2{% endtrans %}</h2>
                            <em>{% trans %}homepage.company.2{% endtrans %},<span> 2013-2014</span></em>
                            <p>{% trans %}homepage.job_description.2{% endtrans %}</p>
                        </div>
                        <div class="item text-center">
                            <h2>{% trans %}homepage.title.3{% endtrans %}</h2>
                            <em>{% trans %}homepage.company.3{% endtrans %},<span> 2015-2017</span></em>
                            <p>{% trans %}homepage.job_description.3{% endtrans %}</p>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 skills text-center">
                    <div class="animate fadeInRight">
                        <div class="heading">
                            <h1>{% trans %}homepage.about_me{% endtrans %}</h1>
                            <p>{% trans %}homepage.about_me_content{% endtrans %}</p>
                        </div>
                    </div>
                    <div class="animate bounceInUp">
                        <div class="row">
                            <div class="col-md-4 col-sm-6">
                                <div class="single-service">
                                    <i class="fa fa-keyboard-o"></i>
                                    <h2>{% trans %}homepage.skills_title.1{% endtrans %}</h2>
                                    <p>{% trans %}homepage.skills_description.1{% endtrans %}</p>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-6">
                                <div class="single-service">
                                    <i class="fa fa-code"></i>
                                    <h2>{% trans %}homepage.skills_title.2{% endtrans %}</h2>
                                    <p>{% trans %}homepage.skills_description.2{% endtrans %}</p>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-6">
                                <div class="single-service">
                                    <i class="fa fa-code-fork"></i>
                                    <h2>{% trans %}homepage.skills_title.3{% endtrans %}</h2>
                                    <p>{% trans %}homepage.skills_description.3{% endtrans %}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

