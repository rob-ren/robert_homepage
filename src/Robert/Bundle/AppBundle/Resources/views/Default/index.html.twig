<!-- contact me -->
<div id="contactmesec" class="row section">
    <div class="container contact">
        <div class="row">
            <div class="col-xs-12 text-center">
                <div class="animate fadeInRight">
                    <div class="heading">
                        <h1>{% trans %}homepage.contact_me{% endtrans %}</h1>
                        <p>{% trans %}homepage.contact.subline{% endtrans %} ğŸ¤—</p>
                    </div>
                </div>
                <div class="col-md-7 col-sm-12 col-xs-12 field">
                    <div class="animate bounceInLeft">
                        <div id="result" style="display: block">
                        </div>
                        <form id="contact-form" onsubmit="return false">
                            <div class="user">
                                <input type="text" name="full_name"
                                       placeholder="{% trans %}homepage.contact.name{% endtrans %}" required/>
                                <i class="fa fa-user" aria-hidden="true"></i></div>
                            <div class="user">
                                <input type="text" name="email_address"
                                       placeholder="{% trans %}homepage.contact.email{% endtrans %}" required/>
                                <i class="fa fa-envelope" aria-hidden="true"></i></div>
                            <textarea name="apply_message"
                                      placeholder="{% trans %}homepage.contact.message{% endtrans %}"></textarea>
                            <input id="btn-submit" type="submit"
                                   value="{% trans %}homepage.contact.submit{% endtrans %}"/>
                        </form>
                    </div>
                </div>
                <div class="col-md-4 col-sm-8 col-xs-12 contact_info">
                    <div class="animate bounceInRight">
                        <h3>{% trans %}homepage.get_in_touch{% endtrans %}</h3>
                        <div class="contact_sec">
                            <ul>
                                <li><i class="fa fa-phone"
                                       aria-hidden="true"></i><span>{% trans %}homepage.contact.phone{% endtrans %}: +61 (0)450 981 126</span>
                                </li>
                                <li><i class="fa fa-envelope-o" aria-hidden="true"></i><span>{% trans %}
                                        homepage.contact.email{% endtrans %}: <a
                                                href="mailto:info@renxubo.me?Subject=Hello%20Ren Xubo" target="_top"> info@renxubo.me</a></span>
                                </li>
                            </ul>
                        </div>
                        <!-- Social Icons -->
                        <div class="contact_icons">
                            <ul>
                                <li><a href="https://www.facebook.com/profile.php?id=100004002800303" target="_blank"
                                       class="fa fb"><i class="fa fa-fw fa-facebook"></i></a></li>
                                <li><a href="https://www.linkedin.com/in/xubo-ren-149266ab/" target="_blank"
                                       class="fa in"><i class="fa fa-fw fa-linkedin"></i></a></li>
                                <li><a href="https://github.com/robertren1" target="_blank" class="fa gh"><i
                                                class="fa fa-fw fa-github"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 footer text-center">
                    <div class="animate fadeInLeft">
                        <p> &copy; 2017 Robert Ren. </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<a href="#." class="cd-top">TOP</a>
